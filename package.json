    "eslint": "^9.17.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-tsdoc": "~0.4.0",
    "jest": "^29.7.0",
    "jest-fixed-jsdom": "0.0.9",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typedoc": "~0.27.6",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "webpack-cli": "^6.0.1",
