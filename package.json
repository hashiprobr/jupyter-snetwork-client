    "eslint": "^9.12.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsdoc": "^50.3.1",
    "eslint-plugin-tsdoc": "~0.3.0",
    "jest": "^29.7.0",
    "jest-fixed-jsdom": "0.0.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typedoc": "~0.26.8",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "webpack-cli": "^5.1.4",
