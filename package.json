{
  "name": "jupyter-snetwork-client",
  "version": "0.0.5",
  "description": "TypeScript client for jupyter-snetwork. This library should not be used on its own.",
  "homepage": "https://github.com/hashiprobr/jupyter-snetwork-client#readme",
  "bugs": {
    "url": "https://github.com/hashiprobr/jupyter-snetwork-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hashiprobr/jupyter-snetwork-client.git"
  },
  "license": "EPL-2.0",
  "author": "Marcelo Hashimoto <marcelo.hashimoto@gmail.com>",
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "webpack --mode production --devtool source-map",
    "doc": "typedoc src --readme typedoc.md --disableSources",
    "start": "webpack serve --mode development --devtool eval-source-map",
    "test": "jest"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "preset": "ts-jest",
    "testEnvironment": "jest-fixed-jsdom"
  },
  "dependencies": {
    "jupyter-jchannel-client": "~0.3.9",
    "pixi.js": "^8.15.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-jsdoc": "^62.0.0",
    "eslint-plugin-tsdoc": "~0.5.0",
    "jest": "^30.2.0",
    "jest-fixed-jsdom": "0.0.11",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typedoc": "~0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3"
  },
  "engines": {
    "node": "24.x"
  }
}