{
  "name": "jupyter-snetwork-client",
  "version": "0.0.3",
  "description": "TypeScript client for jupyter-snetwork. This library should not be used on its own.",
  "homepage": "https://github.com/hashiprobr/jupyter-snetwork-client#readme",
  "bugs": {
    "url": "https://github.com/hashiprobr/jupyter-snetwork-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hashiprobr/jupyter-snetwork-client.git"
  },
  "license": "EPL-2.0",
  "author": "Marcelo Hashimoto <marcelo.hashimoto@gmail.com>",
  "type": "module",
